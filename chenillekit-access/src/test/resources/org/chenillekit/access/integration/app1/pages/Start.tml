<html xmlns="http://www.w3.org/1999/xhtml" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
<head>
		<title>Basic Tests</title>
</head>

<body>

<p>${items.size()} standard test pages</p>

<ul>
		<li t:type="loop" source="items" value="item">
				<t:pagelink id="${item.label}" page="prop:item.pageName">${item.label}</t:pagelink>
				-- ${item.description}
		</li>
</ul>

<span id="no_access">no access</span>
<span t:id="ifNotAuthenticated">
	<span id="not_authenticated">not authenticated</span>
</span>

<span t:id="ifAuthenticated">
	<span id="authenticated">is authenticated</span>
</span>

</body>
</html>
